{
  "name": "opskings-support-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx scripts/db-seed.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:update": "tsx scripts/db-update.ts",
    "zero:cache": "bash -lc 'set -a; source .env.local 2>/dev/null || source .env; set +a; pnpm exec zero-cache-dev'",
    "zero:reset": "bash -lc 'rm -f zero.db*'",
    "generate": "drizzle-zero generate --schema lib/db/schema.ts --format",
    "postinstall": "drizzle-zero generate --schema lib/db/schema.ts --format",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "email:test": "bash -lc 'set -a; source .env.local 2>/dev/null || source .env; set +a; tsx scripts/resend-test.ts \"$@\"' --"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@phosphor-icons/react": "^2.1.10",
    "@rocicorp/zero": "^0.25.6",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "react-tweet": "^3.3.0",
    "recharts": "2.15.4",
    "remark-gfm": "^4.0.1",
    "resend": "^6.6.0",
    "shadcn": "^3.6.2",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "drizzle-zero": "^0.17.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@rocicorp/zero-sqlite3"
    ]
  }
}
